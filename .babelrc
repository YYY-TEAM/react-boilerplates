{
  "presets": ["env", "stage-2", "react"],
  "plugins": ["react-hot-loader/babel", "transform-decorators-legacy"],
  "env": {
    "production": {
      "presets": [["env", { "modules": false, "useBuiltIns": true }], "stage-2", "react"],
      "plugins": [
        "transform-decorators-legacy",
        [
          "transform-react-remove-prop-types",
          {
            "removeImport": true
          }
        ]
      ]
    },
    "test": {
      "presets": [["env"], "stage-2", "react"]
    }
  }
}
