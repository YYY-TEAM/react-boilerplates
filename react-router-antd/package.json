{
  "name": "react-router-antd",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "https://github.com/leohxj/react-boilerplates.git",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.dev.js --hot",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.config.prod.js --display-modules",
    "test": "jest",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx, less, pcss}": [
      "prettier --write"
    ],
    "**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "**/*.*.{less, pcss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "keywords": [
    "bootstrap",
    "webpack4",
    "react"
  ],
  "author": "Leo Hui <leohxj@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "antd": "^3.9.1",
    "classnames": "^2.2.5",
    "moment": "^2.22.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^4.2.2",
    "url-search-params": "^0.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^6.0.1",
    "@commitlint/config-conventional": "^5.2.3",
    "@leohxj/eslint-config-dawn-react": "^0.0.3",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.2",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.15",
    "clean-webpack-plugin": "^0.1.17",
    "commitizen": "^2.9.6",
    "commitlint-config-cz": "^0.7.0",
    "cross-env": "^5.1.3",
    "css-loader": "^1.0.0",
    "cz-customizable": "^5.2.0",
    "eslint": "^4.19.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.9",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "lint-staged": "^5.0.0",
    "mini-css-extract-plugin": "^0.4.0",
    "opn": "^5.3.0",
    "postcss-loader": "^2.0.6",
    "postcss-preset-env": "^5.2.0",
    "prettier": "^1.13.6",
    "prop-types": "^15.6.2",
    "style-loader": "^0.18.2",
    "stylelint": "^9.3.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^3.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "uglifyjs-webpack-plugin": "^2.0.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  }
}
